{
  "expo": {
    "name": "ecommerce-app",
    "slug": "commerce-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_bg.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_c.png",
      "resizeMode": "contain",
      "backgroundColor": "#001AFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_bg.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.prnbbora4_ecommapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}